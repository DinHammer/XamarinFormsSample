<?xml version="1.0" encoding="UTF-8"?>
<ContentView xmlns="http://xamarin.com/schemas/2014/forms" 
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Dlphn.Staff.Customs.Cell.CellMeterDataSendYes">
  <ContentView.Content>
      <StackLayout>
            <Grid>
                <Grid.RowDefinitions>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                    <RowDefinition Height="*"/>
                </Grid.RowDefinitions>
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="4*"/>
                    <ColumnDefinition Width="*"/>
                </Grid.ColumnDefinitions>
                <Label x:Name="lblTarifName" Grid.Row="0" Grid.Column="0"/>
                <Entry x:Name="entry" Grid.Row="1" Grid.Column="0"/>
                <Label x:Name="lblUnitOfMeasurement" Grid.Row="1" Grid.Column="1"/>
                <Label x:Name="lblLastValue" Grid.Row="2" Grid.Column="0" Grid.ColumnSpan="2"/>
            </Grid>
      </StackLayout>
  </ContentView.Content>
</ContentView>